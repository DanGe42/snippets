  <% @snippets.each do |snippet| %>
	
	<div id="outter">  
    <%= snippet.title %>
    <%= truncate(snippet.description, :length => 80) %>
    
    <%= snippet.points %>               
    <%= localize_time(snippet.created_at) + " ago" %>                                                            
    <%= snippet.user.name %>
    <%= link_to 'Show', snippet %>
    <%= link_to 'Destroy', snippet, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>